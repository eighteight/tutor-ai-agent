{
  "name": "tutor-agent",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:e2e": "node e2e-test.js",
    "n8n": "WEBHOOK_URL=https://leigha-chemosynthetic-undetrimentally.ngrok-free.dev n8n start",
    "n8n:import": "cp 'Tutoring Agent Workflow.json' workflow.json && n8n import:workflow --input=workflow.json --update && n8n update:workflow --id='Tutoring Agent Workflow' --active=true",
    "n8n:import-telegram": "cp 'Theatre Cognitive Development Telegram Bot.json' telegram-workflow.json && n8n import:workflow --input=telegram-workflow.json",
    "n8n:import-telegram-simple": "n8n import:workflow --input=telegram-workflow-simple.json",
    "telegram-bot": "python3 telegram-bot.py",
    "ngrok": "ngrok http 5678",
    "lightrag": "python3 lightrag-server.py",
    "lightrag:restart": "pkill -f lightrag-server; python3 lightrag-server.py > lightrag.log 2>&1 &"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "~14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "dotenv": "^17.2.3",
    "n8n": "^1.119.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/d3-selection": "^3.0.11",
    "@types/jasmine": "~4.0.0",
    "@types/node": "18.16.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.2"
  }
}
